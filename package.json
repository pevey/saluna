{
	"name": "sveltekit-vendure-starter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently 'vite dev' 'yarn generate --watch'",
		"build": "shx rm -rf ./build && yarn generate && vite build",
		"preview": "vite preview",
		"test": "yarn test:integration && yarn test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"generate": "graphql-codegen"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.0",
		"@graphql-codegen/client-preset": "^4.1.0",
		"@melt-ui/svelte": "^0.62.1",
		"@parcel/watcher": "^2.3.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.27.1",
		"@tailwindcss/forms": "^0.5.6",
		"@tailwindcss/typography": "^0.5.10",
		"autoprefixer": "^10.4.16",
		"concurrently": "^8.2.2",
		"dotenv": "^16.3.1",
		"formsnap": "^0.2.0",
		"graphql": "^16.8.1",
		"lucide-svelte": "^0.292.0",
		"shx": "^0.3.4",
		"svelte": "^4.2.7",
		"svelte-check": "^3.4.3",
		"svelte-legos": "^0.2.2",
		"svelte-meta-tags": "^3.0.4",
		"sveltekit-search-params": "^1.0.15",
		"sveltekit-superfetch": "^3.0.3",
		"sveltekit-superforms": "^1.8.0",
		"sveltekit-turnstile": "^1.2.0",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "5.0.2",
		"vitest": "0.34.6",
		"zod": "^3.22.4"
	},
	"type": "module"
}
